class Solution {
    public void reverseArray(int arr[]) {
        int[] result=new int[arr.length];
        int j=1;
        for(int i=0;i<arr.length;i++)
        {
            result[i]=arr[arr.length-j];
            j++;
        }
          for(int i=0;i<result.length;i++)
        {
             arr[i]=result[i];
        }
      
    }
}
class Solution {
    public int countEven(int num) {
        int r,count=0;
        for(int n=2;n<=num;n++)
        {
            int sum=0;
            int temp=n;
        while(temp>0)
        {
            r=temp%10;
            temp=temp/10;
            sum=sum+r;
        }
        if(sum%2==0)
        {
            count++;
        }
        }
        return count;
    }
}